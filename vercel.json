{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/flutter.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/main.dart.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@2.0.0"
    }
  },
  "builds": [
    {
      "src": "local_shop_app/pubspec.yaml",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "flutter build web --release --web-renderer html",
        "outputDirectory": "local_shop_app/build/web"
      }
    }
  ]
}
